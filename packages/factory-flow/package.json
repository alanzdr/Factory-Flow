{
  "name": "factory-flow",
  "version": "0.3.0",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanzdr/Factory-Flow.git"
  },
  "author": {
    "name": "Alanderson Zelindro da Rosa",
    "url": "https://github.com/alanzdr"
  },
  "maintainers": [
    {
      "name": "Alanderson Zelindro da Rosa",
      "url": "https://github.com/alanzdr"
    }
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./core": "./dist/core/index.js",
    "./core/*": "./dist/core/*/index.js",
    "./modules": "./dist/modules/index.js",
    "./modules/*": "./dist/modules/*/index.js",
    "./utils": "./dist/utils/index.js",
    "./utils/*": "./dist/utils/*/index.js",
    "./services": "./dist/services/index.js",
    "./services/*": "./dist/services/*/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "core": [
        "dist/core/index.d.ts"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "modules": [
        "dist/modules/index.d.ts"
      ],
      "modules/*": [
        "dist/modules/*"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "utils/*": [
        "dist/utils/*"
      ],
      "services": [
        "dist/services/index.d.ts"
      ],
      "services/*": [
        "dist/services/*"
      ]
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}